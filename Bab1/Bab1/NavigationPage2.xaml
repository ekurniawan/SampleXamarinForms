<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Bab1.NavigationPage2" Title="Halaman 2">
  <ContentPage.ToolbarItems>
    <ToolbarItem Text="Halaman 1" Order="Secondary"
                 Clicked="Navigate" CommandParameter="NavigationPage1" />
    <ToolbarItem Text="Halaman 2" Order="Secondary"
                 Clicked="Navigate" CommandParameter="NavigationPage2" />
  </ContentPage.ToolbarItems>
  <StackLayout>
    <Label Text="Halaman 2" FontSize="Large" />
    <Button Text="Kembali ke Halaman 1" x:Name="btnKembali" />
  </StackLayout>


</ContentPage>